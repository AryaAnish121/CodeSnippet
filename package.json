{
    "name": "code-snippet",
    "displayName": "Code Snippet",
    "description": "This is a simple code generator for almost every language",
    "version": "2.0.0",
    "publisher": "aryaanish",
    "author": {
        "name": "Arya Anish"
    },
    "keywords": [
        "code snippet"
    ],
    "icon": "logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/AryaAnish121/CodeSnippet.git"
    },
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "css",
                "path": "./snippets/snippets.code-snippets"
